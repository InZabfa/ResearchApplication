<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KidsApp.Forms.Home_Researcher.ResearcherNavigation"
             xmlns:pages="clr-namespace:KidsApp.Forms.Home_Researcher"
             NavigationPage.HasNavigationBar="False">
    <MasterDetailPage.Master>
        <pages:ResearcherNavigationMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:ResearcherNavigationDetail />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>